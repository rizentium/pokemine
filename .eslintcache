emon();\r\n  }, [id]);\r\n\r\n  useEffect(() => {\r\n    const speed = pokemon?.stats.filter(stat => stat.stat.name === 'speed')[0].base_stat;\r\n    const speedToSecond = !speed ? 0 : speed*100;\r\n\r\n    setTimeout(() => {\r\n      setMovement([Math.random() * window.innerWidth, Math.random() * window.innerHeight]);\r\n    }, 11000 - speedToSecond)\r\n  }, [movement]);\r\n\r\n\treturn (\r\n\t\t<div>\r\n      {\r\n        loading ?\r\n          (\r\n            <LoadingOutlined />\r\n          ) :\r\n          (\r\n            <CatchPokemonImage\r\n              id=\"pokemon\"\r\n              loading='lazy'\r\n              src={pokemon?.sprites.front_default}\r\n              top={movement[0]}\r\n              left={movement[1]}\r\n            />\r\n          )\r\n      }\r\n    </div>\r\n\t);\r\n}","C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\catch\\CatchPokeballComponent.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\detail\\Detail.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\detail\\DetailBodyComponent.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\detail\\DetailBottomComponent.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\mine\\Mine.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\home\\Home.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\home\\HomeBodyComponent.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\catch\\CatchModalComponent.tsx",[],{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","severity":1,"message":"117","line":33,"column":6,"nodeType":"118","endLine":33,"endColumn":10,"suggestions":"119"},{"ruleId":"116","severity":1,"message":"120","line":42,"column":6,"nodeType":"118","endLine":42,"endColumn":16,"suggestions":"121"},"no-native-reassign",["122"],"no-negated-in-lhs",["123"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'props'. Either include it or remove the dependenc