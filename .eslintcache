iv>\r\n    ) :\r\n    (\r\n      <CatchPokemonImage\r\n        id=\"pokemon\"\r\n        loading='lazy'\r\n        src={props.pokemon.value?.sprites.front_default}\r\n        top={movement[0]}\r\n        left={movement[1]}\r\n      />\r\n    );\r\n}","C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\catch\\CatchPokeballComponent.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\detail\\Detail.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\detail\\DetailBodyComponent.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\detail\\DetailBottomComponent.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\mine\\Mine.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\home\\Home.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\home\\HomeBodyComponent.tsx",[],"C:\\Users\\arif_\\Documents\\mypokemon\\src\\pages\\catch\\CatchModalComponent.tsx",["130","131"],"import { Input, Modal } from \"antd\";\r\nimport React, { BaseSyntheticEvent, useContext, useState } from \"react\";\r\nimport { useHistory } from \"react-router-dom\";\r\nimport { CatchedPokemonInterface } from \"../../interfaces/CatchedPokemon\";\r\nimport { PokemonInterface } from \"../../interfaces/Pokemon\";\r\nimport { setCatchedPokemon } from \"../../storages/CatchedPokemon\";\r\nimport { CatchedPokemonContext } from \"../../stores/CatchedPokemon\";\r\n\r\ninterface CatchModalComponentProps {\r\n  visible: boolean,\r\n  setModalVisible: any,\r\n  pokemon: PokemonInterface | undefined,\r\n  catchedPokemons: CatchedPokemonInterface[];\r\n}\r\n\r\nexport const CatchModalComponent = (props: CatchModalComponentProps) => {\r\n  const [ Nickname, setNickname ] = useState('');\r\n  const [ IsNameAllowed, setIsNameAllowed ] = useState(true);\r\n  const history = useHistory();\r\n\r\n  const actionOnClick = () => {\r\n    props.setModalVisible(!props.visible);\r\n  }\r\n\r\n  const actionOnCatch = async () => {\r\n    const isSuccess = await setCatchedPokemon(